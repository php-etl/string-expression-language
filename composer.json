{
    "name": "php-etl/string-expression-language",
    "description": "This library implements functions for manipulating string data in ExpressionLanguage",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "symfony/expression-language": "^5.2 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "rector/rector": "^0.15",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\StringExpressionLanguage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\StringExpressionLanguage\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev",
            "dev-feature/qualityflow-improvments": "0.3.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
